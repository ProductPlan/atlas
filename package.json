{
  "name": "atlas-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "export": "turbo run export",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "plop": "yarn workspace @productplan/atlas plop"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
